{
    "name": "Tracks Vigilante",
    "manifest_version": 2,
    "version": "1.1",
    "description": "Tracks network calls to Tracks",
    "permissions": [
      "activeTab",
      "webRequest",
      "storage"
    ],
    "icons": {
      "16": "/icons/tv16.png",
      "48": "/icons/tv48.png",
     "128": "/icons/tv128.png"
    },
    "background": {
      "scripts": ["service-worker.js"]
    },
    "browser_action": {
      "default_icon": {
        "16": "/icons/tv16.png",
        "48": "/icons/tv48.png",
       "128": "/icons/tv128.png"
      },
      "default_popup": "popup.html"
    },
    "optional_permissions": [
      "http://pixel.wp.com/",
      "https://pixel.wp.com/",
      "*://*/*"
    ]
  }